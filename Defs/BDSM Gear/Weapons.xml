<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BaseMeleeWeapon_Blunt">
    <defName>BDSM_Paddle</defName>
    <label>paddle</label>
    <description>A wooden paddle for spanking disobedient puppies. (increases puppy training).</description>
    <graphicData>
      <texPath>Weapons/Paddle</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <uiIconScale>1.1</uiIconScale>
    <techLevel>Industrial</techLevel>
    <weaponTags>
      <li>IndustrialMeleeBasic</li>
    </weaponTags>
    <weaponClasses>
      <li>MeleeBlunt</li>
    </weaponClasses>
    <costStuffCount>30</costStuffCount>
    <statBases>
      <WorkToMake>1200</WorkToMake>
      <Mass>2</Mass>
    </statBases>
    <equippedAngleOffset>-65</equippedAngleOffset>
    <stuffCategories>
      <li>Woody</li>
    </stuffCategories>
    <recipeMaker>
      <researchPrerequisites>
        <li>PRPuppy</li>
      </researchPrerequisites>
      <recipeUsers>
        <li>CraftingSpot</li>
      </recipeUsers>
      <displayPriority>-300</displayPriority>
    </recipeMaker>
    <tools>
      <li>
        <label>flat side</label>
        <labelUsedInLogging>false</labelUsedInLogging>
        <capacities>
          <li>PuppyPaddle</li>
        </capacities>
        <power>1.5</power>
        <cooldownTime>2</cooldownTime>
        <chanceFactor>200</chanceFactor>
      </li>
    </tools>
  </ThingDef>


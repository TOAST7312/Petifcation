<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="HatMakeableBase">
    <defName>Apparel_PuppyCollar</defName>
    <label>doggy collar</label>
    <description>A restrictive collar designed to mark ownership and submission. Its tight grip reinforces 'puppy' training and the wearer's servile status.</description>
    <descriptionHyperlinks>
      <HediffDef>Dog_WikiEntry</HediffDef>
    </descriptionHyperlinks>
    <graphicData>
      <texPath>Apparel/Dog/DogCollars/DogCollar_south</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.8</drawSize>
    </graphicData>
    <costStuffCount>25</costStuffCount>
    <stuffCategories>
      <li>Metallic</li>
      <li>Leathery</li>
    </stuffCategories>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToMake>1800</WorkToMake>
      <Mass>0.14</Mass>
      <StuffEffectMultiplierArmor>0.0</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.00</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.00</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <SlaveSuppressionOffset>0.15</SlaveSuppressionOffset>
    </equippedStatOffsets>
    <apparel>
      <slaveApparel>true</slaveApparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
        <li>Neck</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/Dog/DogCollars/DogCollar</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <renderSkipFlags>
        <li>None</li>
      </renderSkipFlags>
      <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
      <canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
      <canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
      <defaultOutfitTags>
        <li>Slave</li>
      </defaultOutfitTags>
      <drawData>
        <defaultData>
          <layer>48</layer>
        </defaultData>
        <dataNorth>
          <layer>68</layer>
        </dataNorth>
      </drawData>
    </apparel>
    <comps>
      <li Class="CompProperties_CauseHediff_Apparel">
        <hediff>doggycollared</hediff>
        <part>Neck</part>
      </li>
    </comps>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
    <recipeMaker>
      <displayPriority>176</displayPriority>
      <researchPrerequisites>
        <li>PRPuppy</li>
      </researchPrerequisites>
    </recipeMaker>
  </ThingDef>

